
<div class="card-body">
    <div class="table-responsive">
      <div class="table-responsive">
        <table class="table align-middle table-nowrap mb-0">
          <thead class="table-light">
            <tr>
               
              <th class="align-middle" scope="col">Order ID</th>
              <th class="align-middle" scope="col">Date</th>
              <th class="align-middle" scope="col">Orderstatus</th>
              <th class="align-middle" scope="col">Option</th>
            </tr>
          </thead>
          <tbody>
            <% response?.forEach(function(response) { %>
            <tr>
              
              <td><a href="#" class="fw-bold"> <%=response._id%></a></td>
        
              <td><%=response.createdAt%></td>
              <td>
                <%=response.OrderStatus%>
              </td>
              <td>
               
                  <!-- <button onclick="cancelorder('<%=response._id%>')" class="btn btn-xs">Cancel</button> -->
                  <a href="/admin/adminorder_details?order=<%=response._id%>" class="btn btn-primary"> ViewOrder Details  </a>
              
              </td>
             
            </tr>
            <% }) %> 
           
          </tbody>
        </table>
      </div>
    </div>
 
  </div>

 


